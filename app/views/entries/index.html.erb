<% if @entries.present? %>
<table class="list">
  <tr>
    <th>参加日</th>
    <th>配置</th>
    <th>サークル名</th>
    <th>作家</th>
    <th>頒布物</th>
  </tr>
  <% @entries.each do |entry| %>
  <tr>
  <td align="right"><%= entry.attend_at %></td>
    <td><%= entry.place %></td>
    <td><%= entry.circle.name %></td>
    <td><%= entry.circle.author %></td>
    <td><% entry.handouts.each do |handout| %>
        <%= handout.goods.name %>
        <%= handout.goods.price %>
        <% end %>
    </td>
  </tr>
  <% end %>
</table>
<% else %>
<p>サークル情報がありません</p>
<% end %>
